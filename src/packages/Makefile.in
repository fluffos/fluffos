include packages

# @configure_input@
@SET_MAKE@

CXX=@CXX@
CXXFLAGS=@CXXFLAGS@
YACC=@YACC@
O=@OBJEXT@
DRIVER_BIN=@DRIVER_BIN@
INSTALL=@INSTALL@
INSTALL_DIR=@bindir@
OPTIMIZE=@OPTIMIZE@

.c.$(O):
	$(CXX) $(CXXFLAGS) -I.. -c -x c++ $*.c

all: $(OBJ)

# special db stuff
db.$(O): db.c
	$(CXX) $(CXXFLAGS) -I.. -I/usr/include/mysql -c -x c++ $*.c

clean:
	rm -f *.$(O)
